<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>



    <record id="portal_ticket_user_rule_extends" model="ir.rule">
        <field name="name">Portal Personal Tickets Extends</field>
        <field name="model_id" ref="website_support.model_website_support_ticket"/>
        <field name="domain_force">['|', ('partner_id','=',user.partner_id.id), ('partner_id.parent_id','=',user.partner_id.id)]</field>
        <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
        <field eval="0" name="perm_unlink"/>
        <field eval="1" name="perm_write"/>
        <field eval="1" name="perm_read"/>
        <field eval="0" name="perm_create"/>
    </record>



  </data>
</odoo>
